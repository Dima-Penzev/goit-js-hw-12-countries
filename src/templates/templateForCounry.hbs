{{#each this}}
  <article class='card'>
    <h1 class='header'>{{name}}</h1>
    <div class='description'>
      <div class="description-items">
          <p>Capital: <span>{{capital}}</span></p>
        <p>Population: <span>{{population}}</span></p>
        <p>Languages:</p>
        <ul>
          {{#each languages}}
            <li>{{this.name}}</li>
          {{/each}}
        </ul>
        </div>
      <img src='{{flag}}' alt='{{name}}' class='flag' width='300' />
    </div>
  </article>
{{/each}}